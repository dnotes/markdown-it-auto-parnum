{
  "name": "markdown-it-base",
  "version": "1.0.0",
  "description": "Base project for development of markdown-it plugins.",
  "main": "index.js",
  "scripts": {
    "build": "make build",
    "coverage": "yarn test && nyc report -r text-lcov | coveralls",
    "lint": "eslint .",
    "pub": "yarn lint && yarn test && make build && make publish",
    "generate-tests": "mocha --reporter ./test/generate-tests.js",
    "test": "nyc --check-coverage --lines 100 mocha"
  },
  "keywords": [
    "markdown-it",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.2.3",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "markdown-it": "^8.4.2",
    "markdown-it-attrs": "^2.4.1",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-testgen": "^0.1.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "terser": "^3.17.0"
  },
  "dependencies": {}
}
